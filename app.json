{
  "expo": {
    "name": "Cred",
    "slug": "cred-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0F9FC"
    },
    "extra": {
      "privyAppId": "cmi0pw3ks00gyky0chb1o43ww",
      "eas": {
        "projectId": "094e526c-6f6f-4b04-ae18-b004a420bbaf"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cred.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0BA5C7"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.cred.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          },
          "android": {
            "minSdkVersion": 23
          }
        }
      ]
    ]
  }
}
